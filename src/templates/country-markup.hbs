<li>
    <h2 class='country-title'>{{name}}</h2>
      <img src='{{flag}}' alt='Flag of {{name}}' width="300">
       <p>Capital: {{capital}}</p>
       <p>Population: {{population}}</p>
       <p>Languages:</p>
       <ul>
        {{#each languages}}
        <li>
            {{name}}
        </li>
        {{/each}}
    </ul>
</li>